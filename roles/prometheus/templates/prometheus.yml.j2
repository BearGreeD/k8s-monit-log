global:
  scrape_interval: {{ prometheus_global.scrape_interval }}
  evaluation_interval: {{ prometheus_global.evaluation_interval }}

scrape_configs:
  - job_name: 'prometheus'
    static_configs:
      - targets: ['localhost:9090']
        
  - job_name: 'node_exporter'
    static_configs:
      - targets: ['localhost:9100']